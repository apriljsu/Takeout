<%- include('./partials/header.ejs') %>
<nav>
  <a href="/restaurants">List of restaurants</a>
  |
  <a href="/users/signout">Sign Out</a>
</nav>
  <body>
    <h1>Add A New Restaurant Here</h1>
    <form action="/restaurants" method="POST">
        <fieldset>
          <label for="cuisine">Cuisine:</label>
          <input type="text" name="cuisine" id="cuisine">
        </fieldset>
        <fieldset>
           <label for="name">Restaurant Name:</label>
           <input type="text" name="name" id="name">
        </fieldset>
        <fieldset>
           <label for="rating">Restaurant Rating:</label>
           <input type="number" name="rating" id="rating">
        </fieldset>
        <fieldset>
           <label for="favDish">Favorite Dishes:</label>
           <input type="text" name="favDish" id="favDish">
        </fieldset>
        <fieldset>
           <label for="img">Img Link:</label>
           <input type="text" name="img" id="img">
        </fieldset><br><br>
        <input type="submit" name="submit" value="submit">
    </form>

  </body>
</html>
